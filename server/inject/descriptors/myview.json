{
  "type": "object",
  "properties": {
    "isMyView": {
      "type": "identifier",
      "control": "exists",
      "selector": ".subtle[href]"
    },
    "viewissueslink": {
      "type": "action",
      "selector": "a[href]",
      "control": "action.link"
    }
  },
  "connect": [
    {
      "match": [
        "isMyView"
      ],
      "action": "viewissueslink"
    }
  ]
}
